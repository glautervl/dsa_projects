    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Java DSA Program",
            "request": "launch",
            "mainClass": "dev.dsa.RunDSA",
            "projectName": "dsa"
        },
        {
            "name": "Python DSA Module",
            "type": "debugpy",
            "request": "launch",
            "module": "dsa",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/python_dsa/src",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/python_dsa/src"
            }
        },
        {
            "name": "Go DSA Program",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/go_dsa/cmd/dsa",
            "cwd": "${workspaceFolder}/go_dsa",
            "env": {},
            "args": []
        }
    ]
}